    "@vueuse/head": "^1.0.25",
    "axios": "^1.3.2",
    "colord": "^2.9.3",
    "echarts": "^5.4.1",
    "nprogress": "^0.2.0",
    "pinia": "^2.0.30",
    "pinia-plugin-persistedstate": "^3.0.2",
    "vue": "^3.2.47",
    "vue-i18n": "^9.2.2",
    "vue-router": "^4.1.6"
